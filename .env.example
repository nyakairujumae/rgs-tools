# RGS Tools App - Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT COMMIT .env TO VERSION CONTROL

# ============================================
# Supabase Configuration
# ============================================
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================r
# Firebase Configuration
# ============================================
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id

# Firebase Android
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key
FIREBASE_ANDROID_APP_ID=your_firebase_android_app_id

# Firebase iOS
FIREBASE_IOS_API_KEY=your_firebase_ios_api_key
FIREBASE_IOS_APP_ID=your_firebase_ios_app_id
FIREBASE_IOS_BUNDLE_ID=your_ios_bundle_id

# Firebase Storage
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket

# ============================================
# Apple Push Notification Service (APNs)
# ============================================
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_KEY_PATH=AuthKey_YOUR_KEY_ID.p8

# ============================================
# FCM Topics
# ============================================
FCM_ADMIN_TOPIC=admin_notifications
FCM_TECHNICIAN_TOPIC=technician_notifications
FCM_NEW_REGISTRATION_TOPIC=new_registrations
FCM_TOOL_ISSUES_TOPIC=tool_issues

# ============================================
# App Configuration
# ============================================
APP_NAME=RGS Tools
APP_VERSION=1.0.0
APP_ENVIRONMENT=development


